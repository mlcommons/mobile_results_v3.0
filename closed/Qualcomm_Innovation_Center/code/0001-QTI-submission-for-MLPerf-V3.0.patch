+        } DUMMYSTRUCTNAME;
+    } ID;
+    UINT8 Flag;
+    USHORT Reserved1;
+    ULONG PowerInMPS0;
+    ULONG PowerSavingToMPS0;
+    ULONGLONG ExitLatencyNs;
+    UCHAR Reserved2[8];
